{
    "manifest_version": 2.0,
    "id": "zabbix_ai_agent",
    "name": "AI Agent",
    "version": "1.0",
    "namespace": "ZabbixAiAgent",
    "author": "Antigravity",
    "url": "http://localhost:8000",
    "description": "AI Powered Assistant for Zabbix",
    "assets": {
        "js": [
            "ai_agent.js"
        ]
    },
    "actions": {
        "zabbix_ai_agent.view": {
            "class": "AiAgentView",
            "view": "module.view"
        },
        "zabbix_ai_agent.dashboard": {
            "class": "AiDashboard",
            "view": "ai_dashboard.view",
            "layout": "layout.htmlpage"
        }
    },
    "menu": [
        {
            "label": "AI Dashboard",
            "action": "zabbix_ai_agent.dashboard",
            "icon": "icon-dashboard"
        }
    ],
    "widgets": {
        "ai_summary": "widgets/AiSummary"
    }
}